r && (\n          <IconButton\n            aria-label=\"add to favorites\"\n            onClick={handleAddToFavorites}\n            disabled={creatingFavorite}\n          >\n            <FavoriteIcon />\n          </IconButton>\n        )} */}\n        {/* <IconButton aria-label=\"share\">\n          <ShareIcon />\n        </IconButton> */}\n\n        <ButtonGroup>\n          <Button\n            component={Link}\n            to={`/produits/${id}`}\n            variant=\"contained\"\n            color=\"secondary\"\n          >\n            Consulter\n          </Button>\n        </ButtonGroup>\n      </CardActions>\n    </Card>\n  );\n}\n\nexport default Produit;\n","/home/mouahamdou/dieunde-client/src/reducers/index.js",[],"/home/mouahamdou/dieunde-client/src/reducers/products.js",[],"/home/mouahamdou/dieunde-client/src/actions/actions.js",["246","247","248","249","250","251","252"],"/home/mouahamdou/dieunde-client/src/api/api.js",[],"/home/mouahamdou/dieunde-client/src/reducers/users.js",[],"/home/mouahamdou/dieunde-client/src/pages/NouveauProduit/NouveauProduit.js",["253","254","255","256","257"],"import {\n  Avatar,\n  Button,\n  Container,\n  FormControl,\n  Grid,\n  InputLabel,\n  makeStyles,\n  MenuItem,\n  Paper,\n  Select,\n  TextareaAutosize,\n  TextField,\n  Typography,\n} from '@material-ui/core';\nimport React, { useEffect, useRef, useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { newProduct } from '../../actions/actions';\nimport AlertMessage from '../../components/AlertMessage/AlertMessage';\nimport LocalOfferIcon from '@material-ui/icons/LocalOffer';\nimport CurrencyFormat from 'react-currency-format';\nimport { marques } from '../../data';\nimport DropzoneComponent from 'react-dropzone-component';\nimport '../../../node_modules/react-dropzone-component/styles/filepicker.css';\nimport '../../../node_modules/dropzone/dist/min/dropzone.min.css';\nimport Cloudinary from '../../components/Cloudinary/Cloudinary';