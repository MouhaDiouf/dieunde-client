scription);\n    formData.append('catégorie', catégorie);\n    formData.append('image', image);\n    formData.append('prix', prix);\n    formData.append('user_id', user.id);\n    dispatch(newProduct(formData));\n  };\n\n  return (\n    <Grid container className={classes.root}>\n      <Grid item md={7} xs={false} sm={5} className={classes.right}></Grid>\n      <Grid\n        item\n        md={5}\n        xs={12}\n        sm={7}\n        component={Paper}\n        elevation={6}\n        className={classes.left}\n      >\n        {productCreated && (\n          <AlertMessage message=\"Product created successfully. We will review and approve it if it's valid\" />\n        )}\n        <Typography variant=\"h4\" className={classes.title}>\n          New Product\n        </Typography>\n        <Avatar className={classes.avatar}>\n          <LocalOfferIcon />\n        </Avatar>\n        <form onSubmit={handleCreateProduct} className={classes.form}>\n          <FormControl className={classes.formControl} fullWidth>\n            <TextField\n              id=\"titre\"\n              type=\"text\"\n              onChange={(e) => {\n                setNom(e.target.value);\n              }}\n              label=\"Product name\"\n              variant=\"outlined\"\n            />{' '}\n            <br />\n            <textarea\n              rows=\"7\"\n              name=\"description\"\n              id=\"description\"\n              onChange={(e) => setDescription(e.target.value)}\n              placeholder=\"Product description\"\n              cols=\"7\"\n            />\n            <br />\n            <TextField\n              type=\"number\"\n              name=\"prix\"\n              id=\"prix\"\n              onChange={(e) => setPrix(e.target.value)}\n              label=\"Price (CFA)\"\n              variant=\"outlined\"\n            />{' '}\n            <br />\n            <TextField\n              type=\"file\"\n              accept=\"image/*\"\n              multiple=