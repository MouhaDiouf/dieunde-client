igin={{\n                  vertical: 'top',\n                  horizontal: 'right',\n                }}\n                open={open}\n                onClose={handleClose}\n              >\n                <MenuItem onClick={handleClose} component={Link} to=\"/profile\">\n                  Profil\n                </MenuItem>\n\n                <MenuItem\n                  onClick={handleClose}\n                  component={Link}\n                  to=\"/vendre-voiture\"\n                >\n                  Vendre\n                </MenuItem>\n                <MenuItem onClick={handleLogout}>Déconnexion</MenuItem>\n              </Menu>\n            </div>\n          )}\n          <DrawerController />\n        </Toolbar>\n      </AppBar>\n    </>\n  );\n}\n\nexport default Header;\n","/home/mouahamdou/dieunde-client/src/pages/Connexion/Connexion.js",[],"/home/mouahamdou/dieunde-client/src/pages/NotFound/NotFound.js",[],"/home/mouahamdou/dieunde-client/src/pages/NouveauProduit/NouveauProduit.js",["225"],"import {\n  Avatar,\n  Button,\n  Chip,\n  CircularProgress,\n  FormControl,\n  Grid,\n  InputLabel,\n  makeStyles,\n  MenuItem,\n  Paper,\n  Select,\n  TextField,\n  Typography,\n} from '@material-ui/core';\nimport React, { useEffect, useRef, useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { newProduct } from '../../actions/actions';\nimport AlertMessage from '../../components/AlertMessage/AlertMessage';\nimport LocalOfferIcon from '@material-ui/icons/LocalOffer';\nimport { marques } from '../../data';\nimport axios from 'axios';\nimport HighlightOffIcon from '@material-ui/icons/HighlightOff';\nimport PhotoLibraryIcon from '@material-ui/icons/PhotoLibrary';\nimport NumberFormat from 'react-number-format';\nimport { useHistory } from 'react-router-dom';\n\nconst useStyles = makeStyles({\n  root: {\n    textAlign: 'center',\n\n    marginBottom: '30px',\n  },\n  fileInput: {\n    display: 'none',\n  },\n  form: {\n    display: 